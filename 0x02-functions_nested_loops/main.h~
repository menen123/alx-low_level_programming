/1.**0. _putchar8*/
#include <stdio.h>
 int main()
 {
 int ch;
 for(ch= '_';ch= 'p';ch= 'u');
 for(ch = 't';ch = 'c';ch = 'h');
 for(ch = 'a';ch;);
 for(ch = 'r';ch;);
 putchar(ch);
 putchar(ch);
 putchar(ch);
 putchar(ch);
 return(0);
 }
/2.**2. 10 x alphabet*/

void print_alphabet_x10(void)
{
  int ch;
  int index = 1;
  while(index <= 10)
{
  for(ch= 'a';ch <= 'z';ch++);
  putchar(ch);
  putchar(index);
  index = index + 1;
}
  return(0);
}
